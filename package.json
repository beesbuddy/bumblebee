{
  "name": "bumblebee-ui",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently \"npm:dev:css\" \"shadow-cljs watch app\"",
    "build": "npm run build:css && shadow-cljs release app",
    "dev:css": "concurrently \"npm:sass:watch\" \"npm:tw:watch\"",
    "build:css": "npm run sass:build && npm run tw:build",
    "sass:watch": "sass --load-path=node_modules src/styles/main.scss resources/public/css/main.pre.css --watch",
    "sass:build": "sass --load-path=node_modules src/styles/main.scss resources/public/css/main.pre.css --no-source-map",
    "tw:watch": "wait-on resources/public/css/main.pre.css && postcss resources/public/css/main.pre.css -o resources/public/css/main.css --watch",
    "tw:build": "postcss resources/public/css/main.pre.css -o resources/public/css/main.css",
    "test:watch": "shadow-cljs watch test",
    "test:ci": "shadow-cljs run :test"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "events": "^3.3.0",
    "mqtt": "^5.14.1",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-scss": "^4.0.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-refresh": "^0.14.2",
    "sass": "^1.93.2",
    "shadow-cljs": "3.2.0",
    "tailwindcss": "^4.1.14",
    "wait-on": "^9.0.1"
  }
}
